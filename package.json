{
  "name": "gmvc-api",
  "version": "1.0.0",
  "description": "API for Grace Medical Village Clinic",
  "scripts": {
    "format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|css|md|mdx|html)\"",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts --fix",
    "types:watch": "tsc -w",
    "start": "nodemon --exec \"serverless offline start -s local\"",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "build": "tsc",
    "debug": "SLS_DEBUG=* && node --inspect && npm run local",
    "deploy:domain": "serverless create_domain",
    "deploy:development": "NODE_ENV=development && serverless deploy -s development",
    "deploy:production": "NODE_ENV=production && serverless deploy -s production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grace-Medical-Village/backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Grace-Medical-Village/backend/issues"
  },
  "homepage": "https://github.com/Grace-Medical-Village/backend#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.9",
    "@types/pg": "^7.14.8",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "aws-sdk": "^2.798.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "serverless": "^2.29.0",
    "serverless-domain-manager": "^4.2.3",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "ts-jest": "^26.4.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "pg": "^8.5.1"
  }
}
