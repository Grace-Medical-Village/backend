{
  "name": "gmvc-api",
  "version": "1.0.0",
  "description": "API for Grace Medical Village Clinic",
  "scripts": {
    "prepare": "husky install",
    "debug": "SLS_DEBUG=* && node --inspect && npm run local",
    "format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|css|md|mdx|html)\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:watch": "eslint . --ext .ts",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --run-in-band",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js jest --run-in-band --watch",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "types": "tsc",
    "types:watch": "tsc --watch",
    "validate": "npm-run-all --parallel format lint types",
    "dev": "ts-node-dev --poll --respawn --require dotenv/config src/index.ts dotenv_config_path=.env.local",
    "start": "ts-node --require dotenv/config src/index.ts dotenv_config_path=.env.local",
    "build": "tsc --project tsconfig-build.json",
    "deploy:development:domain": "serverless create_domain -s development",
    "deploy:development": "serverless deploy -s development",
    "deploy:production:domain": "serverless create_domain -s production",
    "deploy:production": "serverless deploy -s production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grace-Medical-Village/backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Grace-Medical-Village/backend/issues"
  },
  "homepage": "https://github.com/Grace-Medical-Village/backend#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/aws-sdk": "^2.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "aws-sdk": "^2.892.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "serverless": "^2.38.0",
    "serverless-domain-manager": "^4.2.3",
    "serverless-dotenv-plugin": "^3.9.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/http-errors": "^1.8.0",
    "@vendia/serverless-express": "^4.3.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0"
  }
}
