{
  "name": "gmv-patient-api",
  "version": "1.0.0",
  "description": "Patient API for Grace Medical Village",
  "scripts": {
    "build": "tsc",
    "debug": "SLS_DEBUG=* && node --inspect && npm run local",
    "deploy:development": "NODE_ENV=development && serverless deploy -s development",
    "deploy:production": "NODE_ENV=production && serverless deploy -s production",
    "format": "prettier --write \"**/*.+(js|ts|json|css|md|mdx|html)\" --ignore-path .gitignore",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts --fix",
    "local": "nodemon --exec \"serverless offline start -s local\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type:watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grace-Medical-Village/gmv-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Grace-Medical-Village/gmv-backend/issues"
  },
  "homepage": "https://github.com/Grace-Medical-Village/gmv-backend#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.61",
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "aws-sdk": "^2.771.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.0",
    "nodemon": "^2.0.5",
    "prettier": "^2.0.5",
    "serverless-domain-manager": "^4.2.3",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.5.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "serverless": "^1.76.1"
  }
}
